							} // End If
	
						   else if (data_add.status == "FAILURE") {
						   
						   console.log("Could not retrieve country list!");
												  
						  } // End Else If
	
						  }, // End Success Call
	
							error: function (data_add, textStatus, errorThrown) { //In case of error
							 alert(errorThrown);
						 }
					   
						 });
		
		
		}

