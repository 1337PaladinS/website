		<script type="text/javascript">
			runAllForms();

			$(function() {
                
               $("#submit").click(function() {
				// Validation
				$("#login-form").validate({
					// Rules for form validation
					rules : {
						email : {
							required : true,
							email : true
						},
						password : {
							required : true,
							minlength : 3,
							maxlength : 20
						}
					},

					// Messages for form validation
					messages : {
						email : {
							required : 'Please enter your email address',
							email : 'Please enter a VALID email address'
						},
						password : {
							required : 'Please enter your password'
						}
					},

					// Do not change code below
					errorPlacement : function(error, element) {
						error.insertAfter(element.parent());
					},

                   submitHandler: function() {
                   
                    var UserName = $("#username").val();
                    var PassWord = $("#password").val();
                   
                    $.ajax({
                    type: 'POST', 
                    url: "http://54.175.165.80:8080/oneapplication/login/auth", 
                    contentType:"application/json",
                    dataType: "json", 
					data: '{"username": '+ '"'+ UserName +'"' +  ' , "password":' + '"'+ PassWord + '"'+'}',
                    success: function (responseData) {
         
				   
                   if(responseData.status == "SUCCESS") {
                     
                     var user_id = responseData.responseData.userId;
                     setCookie("userid",user_id,1);
                     setCookie("username",UserName,1);
					 setCookie("jsonData",JSON.stringify(responseData),1);
                     var userrole = responseData.responseData.userRole;
                     switch(userrole) {
                       case "ROLE_STUDENT":
                       window.location = "Student-Dashboard.html";
                       break;

                       case "ROLE_COLLEGE":
                       var collegeid = responseData.responseData.userId;
                       //window.location = "College-ProfMgmt.html#profile?collegeid="+ collegeid;
                       window.location = "College-Dashboard.html";
                       break;

                       case "ROLE_ADMIN":
                       window.location = "Admin-Dashboard.html";
                       break;

                       default:
                    } //End Switch

                   } // End If
                      else if (responseData.status == "FAILURE") {
                         $("#message").removeClass('hidden');
                      } // End Else If
                   },
                     error: function (responseData, textStatus, errorThrown) { //In case of error
                     
                     }
                    });
                     }
				    });
                  });
			    });
		    </script>
